{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "plugin:prettier/reccomended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
      "prettier"
    ],
    "rules": {
      "no-use-before-define": "off",
      "prettier/prettier": "error"
    }
}
